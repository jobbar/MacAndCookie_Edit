<Window x:Class="MacAndCookie_Edit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MacAndCookie_Edit"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
            <Button x:Name="btnMacEdit" Height="28" Width="125" Content="修改Mac" Margin="5"
                    Click="btnEditMac_Click"/>
            <Button x:Name="btnCookieClear" Height="28" Width="125" Content="清理Cookie" Margin="5"
                    Click="btnClearCookie_Click"/>
        </StackPanel>
    </Grid>
</Window>
